    "simple-git": "3.24.0",
    "sort-keys": "5.0.0",
    "type-fest": "4.18.3",
    "winston": "3.13.0",
    "xml2js": "^0.6.2",
    "yaml": "2.4.2",
    "yeoman-environment": "4.4.0",
    "yeoman-generator": "7.2.0",
    "yosay": "^3.0.0"
  },
  "devDependencies": {
    "@actions/core": "1.10.1",
    "@node-loaders/esbuildx": "1.4.0",
    "@types/chai": "4.3.11",
    "@types/node": "^20.11.25",
    "@types/sinon-chai": "3.2.12",
    "@types/xml2js": "^0.4.14",
